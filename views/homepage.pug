doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        h1 Custom Object Table
        a(href="/update-cobj") Add to this table
        table(border="1")
            thead
                tr
                    th Plate
                    th Name
                    th Color
            tbody
                each car in data
                    tr
                        td #{car.properties.plate}
                        td #{car.properties.name}
                        td #{car.properties.color}
